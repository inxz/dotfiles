[user]
	name = NAME
	email = MAIL
[core]
	autocrlf = false
	fileMode = false
[push]
	default = current
[branch]
	autosetuprebase = always
[alias]
	st = status -sb
	lg = log --pretty=format:'%Cred%h%Creset -%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' -10
	ll = log --oneline --graph --decorate --all
	vimdiff = difftool -t vimdiff
	co = checkout
	ci = commit
	root = rev-parse --show-toplevel
[color]
	branch = auto
	diff = auto
	status = auto
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red
	new = cyan
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
